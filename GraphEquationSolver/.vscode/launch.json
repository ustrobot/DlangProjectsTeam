{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug GraphEquationSolver",
            "type": "gdb",
            "request": "launch",
            "target": "${workspaceFolder}/GraphEquationSolver",
            "arguments": "",
            "cwd": "${workspaceFolder}",
            "gdbpath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
