{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug GraphEquationSolver",
            "type": "gdb",
            "request": "launch",
            "target": "${workspaceFolder}/GraphEquationSolver",
            "arguments": "",
            "cwd": "${workspaceFolder}",
            "gdbpath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build debug",
            "postDebugTask": "",
            "console": "integratedTerminal",
            "internalConsoleOptions": "openOnSessionStart",
            "suppressMultipleWarnings": true
        },
        {
            "name": "Launch GraphEquationSolver",
            "type": "gdb",
            "request": "launch",
            "target": "${workspaceFolder}/GraphEquationSolver",
            "arguments": "",
            "cwd": "${workspaceFolder}",
            "gdbpath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build",
            "postDebugTask": "",
            "console": "integratedTerminal",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Attach to Process",
            "type": "gdb",
            "request": "attach",
            "target": "${workspaceFolder}/GraphEquationSolver",
            "gdbpath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "console": "integratedTerminal"
        }
    ]
}
